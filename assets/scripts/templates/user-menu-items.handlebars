{{log 'items is' items}}
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID#</th>
      <th>Category</th>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>
    {{#each items as |item|}}
    <tr>
      <td>{{item.id}}</td>
      <td>{{item.category}}</td>
      <td>{{item.name}}</td>
      <td>{{item.description}}</td>
      <td>{{item.price}}</td>
      <td><button id="{{item.id}}-order" class="btn btn-primary edit"> Order </button></td>
    </tr>
    {{/each}}
  </tbody>
</table>
